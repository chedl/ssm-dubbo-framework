	        this.task = task;  
	    }  

		public void run() {
			task.fun();
		}
		

		public static void main(String[] args) {
			Task taska = new Task();
			Test t1 = new Test(taska);
//			Test t2 = new Test(taska);
			t1.start();
//			t2.start();
		}
}