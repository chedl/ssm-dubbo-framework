package com.chedl.controller;

public class Test extends Thread{
	
		private Sync sync;  
		  
	    public MyThread(Sync sync) {  
	        this.sync = sync;  
	    }  

		public void run() {
			Task task = new Task();
			task.fun();
		}
		

		public static void main(String[] args) {
			Test t1 = new Test();
			Test t2 = new Test();
			t1.start();
			t2.start();
		}
}