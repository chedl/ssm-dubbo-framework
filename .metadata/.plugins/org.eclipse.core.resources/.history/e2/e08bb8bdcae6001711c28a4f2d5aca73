package com.chedl.controller;

public class Test extends Thread{
	
		private Task task;  
		  
	    public Test(Task task) {  
	        this.task = task;  
	    }  

		public void run() {
			task.fun();
		}
		

		Task task = new Task();
		public static void main(String[] args) {
			Test t1 = new Test(task);
			Test t2 = new Test(task);
			t1.start();
			t2.start();
		}
}