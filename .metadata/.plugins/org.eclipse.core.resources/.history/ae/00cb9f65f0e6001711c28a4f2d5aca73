package com.chedl.controller;

public class MyThread extends Thread{
	
		private Task task;  
		  
	    public MyThread(Task task) {  
	        this.task = task;  
	    }  

		public void run() {
			task.fun();
		}
		
		public static void main(String[] args) {
			Task task = new Task();
			MyThread t1 = new MyThread(task);
			MyThread t2 = new MyThread(task);
			t1.start();
			t2.start();
		}
}